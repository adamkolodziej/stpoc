	<relations>
		<relation code="Html5VideoComponentTimeUpdateEventToHtml5VideoComponentRelation" localized="false">
			<sourceElement type="Html5VideoComponentTimeUpdateEvent" qualifier="timeUpdateEvents" cardinality="many"/>
			<targetElement type="Html5VideoComponent" qualifier="html5VideoComponent" cardinality="one"/>
		</relation>

		<!--CECS-79 ProductCarouselComponent and FixedProductSetComponent -->
		<relation code="ProductsForAbstractProductCollectionComponent" generate="true" localized="false" autocreate="true">
			<deployment table="ProdsProdCollCmpRels" typecode="13002"/>
			<sourceElement qualifier="productCollectionComponents" type="AbstractProductCollectionComponent" cardinality="many">
				<modifiers read="true" write="true" search="true" optional="true"/>
			</sourceElement>
			<targetElement qualifier="products" type="Product" cardinality="many" collectiontype="list" ordered="true">
				<modifiers read="true" write="true" search="true" optional="true"/>
			</targetElement>
		</relation>
		<relation code="CategoriesForAbstractProductCollectionComponent" generate="true" localized="false" autocreate="true">
			<deployment table="CatsProdCollCmpRels" typecode="13003"/>
			<sourceElement qualifier="productCollectionComponents" type="AbstractProductCollectionComponent" cardinality="many">
				<modifiers read="true" write="true" search="true" optional="true"/>
			</sourceElement>
			<targetElement qualifier="categories" type="Category" cardinality="many" collectiontype="list" ordered="true">
				<modifiers read="true" write="true" search="true" optional="true"/>
			</targetElement>
		</relation>

		<!-- CECS-154 Content Top Links Component -->
		<relation code="CmsLinkComponentsForSimpleNavigationBarComponent" generate="true" localized="false" autocreate="true">
			<sourceElement qualifier="simpleNavigationBarComponent" type="SimpleNavigationBarComponent" cardinality="one" />
			<targetElement qualifier="links" type="CmsLinkComponent" cardinality="many" collectiontype="list" ordered="true" />
		</relation>

		<relation code="BlogArticlesForPage" localized="false">
			<sourceElement qualifier="blogArticles" type="BlogArticle" cardinality="many"/>
			<targetElement  qualifier="blogPage" type="BlogPage" cardinality="one"/>
		</relation>
	</relations>

