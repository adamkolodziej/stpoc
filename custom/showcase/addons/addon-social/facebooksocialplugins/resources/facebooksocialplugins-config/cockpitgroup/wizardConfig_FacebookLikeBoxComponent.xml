<?xml version="1.0" encoding="UTF-8"?>

<wizard-config showPrefilledValues="true" selectMode="true" createMode="true">
	<displayed-properties>
		<group qualifier="General" visible="true" initially-opened="false">
			<label key="cockpit.config.label.General" />

			<property qualifier="CMSItem.name"/>
			<property qualifier="CMSItem.catalogVersion"/>
			<property qualifier="AbstractCMSComponent.visible"/>
		</group>
		
		<group qualifier="editorial" visible="true" initially-opened="true">
			<label key="config.general.editorial" />

			<property qualifier="FacebookLikeBoxComponent.facebookPageUrl" />
		</group>
	
		<group qualifier="layout" visible="true" initially-opened="true">
			<label key="cockpit.config.label.Layout" />

			<property qualifier="FacebookLikeBoxComponent.width" />
			<property qualifier="FacebookLikeBoxComponent.height" />
			<property qualifier="FacebookLikeBoxComponent.showFaces" />
			<property qualifier="FacebookLikeBoxComponent.showHeader" />
			<property qualifier="FacebookLikeBoxComponent.showStream" />
			<property qualifier="FacebookLikeBoxComponent.forceWall" />
		
			<property qualifier="FacebookLikeBoxComponent.colorScheme" />
			<property qualifier="FacebookLikeBoxComponent.borderColor" />
		</group>
		
	</displayed-properties>
</wizard-config>