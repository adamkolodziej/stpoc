<items xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="items.xsd">

    <itemtypes>
        <itemtype code="AbstractOrder" generate="false" autocreate="false">
            <attributes>
                <attribute qualifier="deliveryDate" type="java.util.Date">
                    <description>Delivery Date for the Order</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="sourcePackageCode" type="java.lang.String">
                    <description>Source Package Name</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="sourceProductCode" type="java.lang.String">
                    <description>Source Product Code</description>
                    <persistence type="property"/>
                </attribute>
            </attributes>
        </itemtype>
    </itemtypes>
</items>
