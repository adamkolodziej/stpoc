# vim:encoding=utf-8
$productCatalog=sptelProductCatalog
$productVersion=Staged
$catalogVersion=catalogVersion(catalog(id[default=$productCatalog]),version[default=$productVersion])[default=$productCatalog:$productVersion]

$maxThreads=3

INSERT_UPDATE MediaConversionCronJob ; code[unique=true]             ; job(code)          ; sessionLanguage(isocode) ; maxThreads[default=$maxThreads] ; $catalogVersion
                                     ; sptelMediaConversionCronjob ; mediaConversionJob ; en                       ;

INSERT_UPDATE ExtractMediaMetaDataCronJob ; code[unique=true]                  ; job(code)               ; sessionLanguage(isocode) ; maxThreads[default=$maxThreads] ; $catalogVersion
                                          ; sptelExtractMediaMetaDataCronJob ; extractMediaMetaDataJob ; en                       ;
