    </bean>

    <bean id="editContractUpdateCartEntryMethodHookMergeDirective" depends-on="commerceUpdateCartEntryMethodHooks" parent="listMergeDirective">
        <property name="add" ref="editContractCartMethodsHook"/>
    </bean>

