<?xml version="1.0" encoding="UTF-8"?>

<wizard-config showPrefilledValues="true" selectMode="true" createMode="true">
	<displayed-properties>
		<group qualifier="General" visible="true" initially-opened="false">
			<label key="cockpit.config.label.General" />

			<property qualifier="CMSItem.name"/>
			<property qualifier="CMSItem.catalogVersion"/>
			<property qualifier="AbstractCMSComponent.visible"/>
		</group>
		
		<group qualifier="editorial" visible="true" initially-opened="true">
			<label key="config.general.editorial" />
			<property qualifier="FacebookOAuth2LoginComponent.labelLogin" />
			<property qualifier="FacebookOAuth2LoginComponent.labelLogout" />
		</group>
	
		<group qualifier="layout" visible="true" initially-opened="true">
			<label key="cockpit.config.label.Layout" />

			<property qualifier="FacebookOAuth2LoginComponent.showImage" />
			<property qualifier="FacebookOAuth2LoginComponent.showName" />
			<property qualifier="FacebookOAuth2LoginComponent.size" />
		</group>
	
		<group qualifier="permissions" visible="true" initially-opened="true">
			<label key="cockpit.config.label.Permissions" />
		
			<property qualifier="FacebookOAuth2LoginComponent.userPermissions" />	
			<property qualifier="FacebookOAuth2LoginComponent.friendsPermissions" />	
			<property qualifier="FacebookOAuth2LoginComponent.extendedPermissions" />	
			<property qualifier="FacebookOAuth2LoginComponent.pagePermissions" />	
		</group>
		
	</displayed-properties>
</wizard-config>