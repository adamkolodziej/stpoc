<?xml version="1.0" encoding="UTF-8"?>

<wizard-config showPrefilledValues="true" selectMode="true" createMode="true">
	<displayed-properties>
		<group qualifier="General" visible="true" initially-opened="true">
			<label key="cockpit.config.label.General" />

			<property qualifier="CMSItem.name"/>
			<property qualifier="CMSItem.catalogVersion"/>
			<property qualifier="AbstractCMSComponent.visible"/>
		</group>

		<group qualifier="visibility" visible="true" initially-opened="true">
			<label key="config.general.contextvisibility" />
		
			<property qualifier="FacebookSuggestedProductCarouselComponent.socialContentPopulationRule" />
		</group>
	
		<group qualifier="editorial" visible="true" initially-opened="true">
			<label key="config.general.editorial" />

			<property qualifier="FacebookSuggestedProductCarouselComponent.title" />
			
			<property qualifier="FacebookSuggestedProductCarouselComponent.categories" />
			<property qualifier="FacebookSuggestedProductCarouselComponent.categoriesOperator" />
			<property qualifier="FacebookSuggestedProductCarouselComponent.fullCategoryPath" />
		</group>
		
	</displayed-properties>
</wizard-config>