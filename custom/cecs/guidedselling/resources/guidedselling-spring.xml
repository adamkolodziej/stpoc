        <property name="bundleRuleService" ref="bundleRuleService"/>
        <property name="percentPriceBundleRuleService" ref="percentPriceBundleRuleService"/>
        <property name="priceDataFactory" ref="priceDataFactory"/>
    </bean>

    <alias name="defaultBundleDependenciesPopulator" alias="bundleDependenciesPopulator" />
