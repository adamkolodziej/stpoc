        <property name="bundleTemplateService" ref="bundleTemplateService"/>
        <property name="populators">
            <list>
                <ref bean="bundleComponentPopulator"/>
                <ref bean="bundleSelectionPopulator"/>
                <ref bean="bundleDependenciesPopulator"/>
                <ref bean="bundleDisableRulesPopulator"/>
                <ref bean="bundleTotalsPopulator"/>
            </list>
        </property>
        <property name="targetClass" value="com.hybris.showcase.guidedselling.data.BundleOfferData" />
    </bean>

    <alias name="defaultBundleComponentPopulator" alias="bundleComponentPopulator" />
    <bean id="defaultBundleComponentPopulator" class="com.hybris.showcase.guidedselling.populators.BundleComponentPopulator">
        <property name="commercePriceService" ref="subscriptionCommercePriceService"/>
