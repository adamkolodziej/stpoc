<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<config xmlns="http://www.hybris.com/cockpit/config">
	
	<context merge-by="module" parent="auto" component="explorer-tree">
        <explorer-tree:explorer-tree xmlns:explorer-tree="http://www.hybris.com/cockpitng/config/explorertree">
              <!-- <explorer-tree:navigation-node id="hmc.group.mobiletricast.mobiletricastSection"> -->  
             <explorer-tree:navigation-node id="hmc.group.emsextras.emsextrasSection">     
                <explorer-tree:type-node code="MobileEntitlementToken" id="MobileEntitlementToken"/>
            </explorer-tree:navigation-node>
        </explorer-tree:explorer-tree>
    </context>

     <context merge-by="type" parent="GenericItem" type="MobileEntitlementToken" component="listview">
        <list-view:list-view xmlns:list-view="http://www.hybris.com/cockpitng/component/listView">
        	<list-view:column qualifier="customer"/>
        	<list-view:column qualifier="token"/>
        	<list-view:column qualifier="used"/>
        </list-view:list-view>
    </context>
    <context merge-by="type" parent="auto" type="MobileEntitlementToken" component="editor-area">
        <editorArea:editorArea xmlns:editorArea="http://www.hybris.com/cockpitng/component/editorArea">
            <editorArea:tab name="hmc.group.mobiletricast.mobiletricastSection.General">
                <editorArea:section name="hmc.group.mobiletricast.mobiletricastSection.General">
                    <editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="pk"/>
                    <editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="creationtime"/>
                    <editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="customer"/>
                    <editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="token"/>
                    <editorArea:attribute xmlns="http://www.hybris.com/cockpitng/component/editorArea" qualifier="used"/>
                </editorArea:section>
            </editorArea:tab>
        </editorArea:editorArea>
    </context>
</config>
