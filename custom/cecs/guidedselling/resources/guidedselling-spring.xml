    </bean>

    <alias name="defaultBundlePackageConverter" alias="bundlePackageConverter" />
    <bean id="defaultBundlePackageConverter" parent="abstractPopulatingConverter">
        <property name="populators">
            <ref bean="bundlePackagePopulator" />
        </property>
        <property name="targetClass" value="com.hybris.showcase.guidedselling.data.BundlePackageData" />
    </bean>
