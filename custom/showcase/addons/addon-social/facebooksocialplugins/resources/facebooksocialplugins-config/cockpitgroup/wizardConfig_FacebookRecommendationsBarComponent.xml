<?xml version="1.0" encoding="UTF-8"?>

<wizard-config showPrefilledValues="true" selectMode="true" createMode="true">
	<displayed-properties>
		<group qualifier="General" visible="true" initially-opened="false">
			<label key="cockpit.config.label.General" />

			<property qualifier="CMSItem.name"/>
			<property qualifier="CMSItem.catalogVersion"/>
			<property qualifier="AbstractCMSComponent.visible"/>
		</group>
		
		<group qualifier="editorial" visible="true" initially-opened="true">
			<label key="config.general.editorial" />

			<property qualifier="FacebookRecommendationsBarComponent.url" />
			<property qualifier="FacebookRecommendationsBarComponent.domains" />
			<property qualifier="FacebookRecommendationsBarComponent.maxAge" />
		</group>
	
		<group qualifier="layout" visible="true" initially-opened="true">
			<label key="cockpit.config.label.Layout" />

			<property qualifier="FacebookRecommendationsBarComponent.side" />
			<property qualifier="FacebookRecommendationsBarComponent.numRecommendations" />
			<property qualifier="FacebookRecommendationsBarComponent.verbToDisplay" />
		</group>
		
		<group qualifier="activation" visible="true" initially-opened="true">
			<label key="cockpit.config.label.Activation" />
			<property qualifier="FacebookRecommendationsBarComponent.readTime" />
			<property qualifier="FacebookRecommendationsBarComponent.pageScrollPercentage" />			
		</group>
	
		<group qualifier="tracking" visible="true" initially-opened="true">
			<label key="cockpit.config.label.Tracking" />
			
			<property qualifier="FacebookRecommendationsBarComponent.ref" />
		</group>
		
	</displayed-properties>
</wizard-config>